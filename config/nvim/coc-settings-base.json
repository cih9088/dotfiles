{
  "coc.preferences.jumpCommand": "CocSplitIfNotOpen",
  "coc.preferences.formatOnType": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,

  "coc.source.file.ignoreHidden": false,
  "coc.source.tmuxcomplete.shortcut": "T",

  "coc.source.emoji.filetypes": [
    "markdown",
    "gitcommit"
  ],

  "suggest.completionItemKindLabels": {
    "function": "f \uf794",
    "method": "f \uf6a6",
    "variable": "v \uf71b",
    "constant": "v \uf8ff",
    "struct": "S \ufb44",
    "class": "C \uf0e8",
    "interface": "I \ufa52",
    "text": "v \ue612",
    "enum": "E \uf435",
    "enumMember": "m \uf02b",
    "module": "M \uf668",
    "color": "v \ue22b",
    "property": "m \ufab6",
    "field": "m \uf93d",
    "unit": "U \uf475",
    "file": "F \uf471",
    "value": "v \uf8a3",
    "event": "E \ufacd",
    "folder": "F \uf115",
    "keyword": "k \uf893",
    "snippet": "S \uf64d",
    "operator": "O \uf915",
    "reference": "r \uf87a",
    "typeParameter": "T \uf278",
    "default": "d \uf29c"
  },

  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",

  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "diagnostic.hintSign": "•",

  "signature.preferShownAbove": false,

  "codeLens.enable": true,

  "snippets.ultisnips.directories": [
    "UltiSnips",
    "snips",
    "gosnippets/UltiSnips"
  ],
  "snippets.loadFromExtensions": false,

  "python.formatting.provider": "black",

  "yaml.format.enable": true,
  "yaml.schemas": {
    "kubernetes": ["/*"],
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": [
      "/docker-compose.y*ml"]
  },

  "languageserver": {
  }
 }
